{
  "name": "savings-dashboard",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "node ./node_modules/.bin/remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate dev --name init && prisma db seed",
    "start": "remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@node-rs/bcrypt": "^1.6.2",
    "@prisma/client": "^4.12.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@remix-run/vercel": "^1.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/eslint": "^8.37.0",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "c8": "^7.13.0",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "happy-dom": "^8.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "prisma": "^4.12.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "version": "1.0.0"
}

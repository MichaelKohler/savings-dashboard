{
  "name": "savings-dashboard",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "astro check && astro build",
    "change-password": "node --experimental-strip-types --env-file=.env scripts/change-password.ts",
    "create-user": "node --experimental-strip-types --env-file=.env scripts/create-user.ts",
    "dev": "astro dev",
    "dev:host": "astro dev --host",
    "dev:deps": "docker compose -f docker-compose.dev.yml up -d",
    "dev:deps:stop": "docker compose -f docker-compose.dev.yml stop",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "prisma generate",
    "setup": "prisma generate && prisma db push && prisma db seed",
    "start": "node ./dist/server/entry.mjs",
    "test": "vitest",
    "test:e2e": "npm run test:e2e:setup && npm run test:e2e:run && npm run test:e2e:teardown",
    "test:e2e:report": "playwright show-report",
    "test:e2e:run": "node --env-file=.env.e2e ./node_modules/.bin/playwright test",
    "test:e2e:setup": "docker compose -f docker-compose.e2e.yml up -d && sh -c 'for i in {1..30}; do if node --env-file=.env.e2e ./node_modules/.bin/prisma db push --accept-data-loss 2>/dev/null; then break; fi; echo \"Waiting for PostgreSQL ($i/30)...\"; sleep 2; done' && node --env-file=.env.e2e ./node_modules/.bin/prisma db seed",
    "test:e2e:teardown": "docker compose -f docker-compose.e2e.yml down -v",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/node": "^9.0.0",
    "@astrojs/react": "^4.0.0",
    "@node-rs/bcrypt": "^1.10.7",
    "@prisma/client": "^6.19.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.0.0",
    "iron-session": "^8.0.0",
    "isbot": "^5.1.33",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "tiny-invariant": "^1.3.3",
    "typescript-eslint": "^8.53.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "c8": "^10.1.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "happy-dom": "^20.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^6.19.2",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "prisma": {
    "seed": "node --experimental-strip-types prisma/seed.ts"
  },
  "version": "1.0.0"
}

{
  "name": "savings-dashboard",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "change-password": "node --experimental-strip-types --env-file=.env scripts/change-password.ts",
    "create-user": "node --experimental-strip-types --env-file=.env scripts/create-user.ts",
    "dev": "react-router dev",
    "dev:host": "react-router dev --host",
    "dev:deps": "docker compose -f docker-compose.dev.yml up -d",
    "dev:deps:stop": "docker compose -f docker-compose.dev.yml stop",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "prisma generate",
    "setup": "prisma generate && prisma db push && prisma db seed",
    "start": "react-router-serve ./build/server/index.js",
    "test": "vitest",
    "test:e2e": "npm run test:e2e:setup && npm run test:e2e:run && npm run test:e2e:teardown",
    "test:e2e:report": "playwright show-report",
    "test:e2e:run": "node --env-file=.env.e2e ./node_modules/.bin/playwright test",
    "test:e2e:setup": "docker compose -f docker-compose.e2e.yml up -d && sh -c 'for i in {1..30}; do if node --env-file=.env.e2e ./node_modules/.bin/prisma db push --accept-data-loss 2>/dev/null; then break; fi; echo \"Waiting for PostgreSQL ($i/30)...\"; sleep 2; done' && node --env-file=.env.e2e ./node_modules/.bin/prisma db seed",
    "test:e2e:teardown": "docker compose -f docker-compose.e2e.yml down -v",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@node-rs/bcrypt": "^1.10.7",
    "@prisma/client": "^6.17.1",
    "@react-router/node": "^7.5.1",
    "@react-router/serve": "^7.9.4",
    "@tailwindcss/postcss": "^4.1.14",
    "isbot": "^5.1.31",
    "postcss": "^8.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "recharts": "^3.3.0",
    "tiny-invariant": "^1.3.3",
    "typescript-eslint": "^8.46.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.9.4",
    "@react-router/fs-routes": "^7.9.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.1.2",
    "c8": "^10.1.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.17.1",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2",
    "vitest-mock-extended": "^3.1.0",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "prisma": {
    "seed": "node --experimental-strip-types prisma/seed.ts"
  },
  "version": "1.0.0"
}
